                            <td width="5%" valign="top" align="center"><font size="1" face="Verdana">@item.SETNO</font></td>
                            <td width="8%" valign="top" align="center">
                                @if (!string.IsNullOrEmpty(item.FILE_1.Split("/")[1]) && !string.IsNullOrEmpty(item.FILE_1.Split("/")[1]))
                                {
                                    var photoUrl = Url.Content("~/ReadWriteData/") + item.FILE_1;
                                    <a href="@photoUrl" target="_blank">@photoUrl</a>
                                }
                                else{
                                    @*<a href="@photoUrl" target="_blank"></a>*@
                                }

