@model IBS.Models.Reports.IEICPhotoEnclosedModelReport

<div>

    <h3 class="center">@ViewBag.Regions</h3>

    <div class="tableOuter">
        <table class="borderTable" cellpadding="0" cellspacing="0">
            <tbody>
                <tr>
                    <th width="2%" valign="top" align="center"><b><font size="1" face="Verdana">S.No.</font></b></th>
                    <th width="5%" valign="top" align="center"><b><font size="1" face="Verdana">CASE NO.</font></b></th>
                    <th width="5%" valign="top" align="center"><b><font size="1" face="Verdana">CALL RECV DT</font></b></th>
                    <th width="3%" valign="top" align="center"><b><font size="1" face="Verdana">CALL SNO.</font></b></th>
                    <th width="5%" valign="top" align="center"><b><font size="1" face="Verdana">BK NO.</font></b></th>
                    <th width="5%" valign="top" align="center"><b><font size="1" face="Verdana">SET No.</font></b></th>
                    <th width="8%" valign="top" align="center"><b><font size="1" face="Verdana">Photo 1</font></b></th>
                    <th width="8%" valign="top" align="center"><b><font size="1" face="Verdana">Photo 2</font></b></th>
                    <th width="8%" valign="top" align="center"><b><font size="1" face="Verdana">Photo 3</font></b></th>
                    <th width="8%" valign="top" align="center"><b><font size="1" face="Verdana">Photo 4</font></b></th>
                    <th width="8%" valign="top" align="center"><b><font size="1" face="Verdana">Photo 5</font></b></th>
                    <th width="8%" valign="top" align="center"><b><font size="1" face="Verdana">Photo 6</font></b></th>
                    <th width="8%" valign="top" align="center"><b><font size="1" face="Verdana">Photo 7</font></b></th>
                    <th width="8%" valign="top" align="center"><b><font size="1" face="Verdana">Photo 8</font></b></th>
                    <th width="8%" valign="top" align="center"><b><font size="1" face="Verdana">Photo 9</font></b></th>
                    <th width="8%" valign="top" align="center"><b><font size="1" face="Verdana">Photo 10</font></b></th>
                </tr>
                @if (Model != null && Model.lstlistSubmittedPhotobyIE != null && Model.lstlistSubmittedPhotobyIE.Count > 0)
                {
                    var SrNo = 0;
                    @foreach (var item in Model.lstlistSubmittedPhotobyIE)
                    {
                        SrNo = SrNo + 1;
                        <tr>
                            <td width="2%" valign="top" align="center"><font size="1" face="Verdana">@SrNo</font></td>
                            <td width="5%" valign="top" align="Left"><font size="1" face="Verdana">@item.CaseNo</font></td>
                            <td width="5%" valign="top" align="center"><font size="1" face="Verdana">@item.CallRecDT</font></td>
                            <td width="3%" valign="top" align="center"><font size="1" face="Verdana">@item.CallSno</font></td>
                            <td width="5%" valign="top" align="center"><font size="1" face="Verdana">@item.BKNO</font></td>
                            <td width="5%" valign="top" align="center"><font size="1" face="Verdana">@item.SETNO</font></td>
                            <td width="8%" valign="top" align="center">
                                @if (!string.IsNullOrEmpty(item.FILE_1))
                                {
                                    var photoUrl = Url.Content("~/ReadWriteData/IC_PHOTOS/") + item.FILE_1;
                                    <a href="@photoUrl" target="_blank">@item.FILE_1</a>
                                }
                            </td>
                            <td width="8%" valign="top" align="center">
                                @if (!string.IsNullOrEmpty(item.FILE_2))
                                {
                                    var photoUrl = Url.Content("~/ReadWriteData/IC_PHOTOS/") + item.FILE_2;
                                    <a href="@photoUrl" target="_blank">@item.FILE_2</a>
                                }

                            </td>
                            <td width="8%" valign="top" align="center">
                                @if (!string.IsNullOrEmpty(item.FILE_3))
                                {
                                    var photoUrl = Url.Content("~/ReadWriteData/IC_PHOTOS/") + item.FILE_3;
                                    <a href="@photoUrl" target="_blank">@item.FILE_3</a>
                                }

                            </td>
                            <td width="8%" valign="top" align="center">
                                @if (!string.IsNullOrEmpty(item.FILE_4))
                                {
                                    var photoUrl = Url.Content("~/ReadWriteData/IC_PHOTOS/") + item.FILE_4;
                                    <a href="@photoUrl" target="_blank">@item.FILE_4</a>
                                }

                            </td>
                            <td width="8%" valign="top" align="center">
                                @if (!string.IsNullOrEmpty(item.FILE_5))
                                {
                                    var photoUrl = Url.Content("~/ReadWriteData/IC_PHOTOS/") + item.FILE_5;
                                    <a href="@photoUrl" target="_blank">@item.FILE_5</a>
                                }

                            </td>
                            <td width="8%" valign="top" align="center">
                                @if (!string.IsNullOrEmpty(item.FILE_6))
                                {
                                    var photoUrl = Url.Content("~/ReadWriteData/IC_PHOTOS/") + item.FILE_6;
                                    <a href="@photoUrl" target="_blank">@item.FILE_6</a>
                                }

                            </td>
                            <td width="8%" valign="top" align="center">
                                @if (!string.IsNullOrEmpty(item.FILE_7))
                                {
                                    var photoUrl = Url.Content("~/ReadWriteData/IC_PHOTOS/") + item.FILE_7;
                                    <a href="@photoUrl" target="_blank">@item.FILE_7</a>
                                }

                            </td>
                            <td width="8%" valign="top" align="center">
                                @if (!string.IsNullOrEmpty(item.FILE_8))
                                {
                                    var photoUrl = Url.Content("~/ReadWriteData/IC_PHOTOS/") + item.FILE_8;
                                    <a href="@photoUrl" target="_blank">@item.FILE_8</a>
                                }

                            </td>
                            <td width="8%" valign="top" align="center">
                                @if (!string.IsNullOrEmpty(item.FILE_9))
                                {
                                    var photoUrl = Url.Content("~/ReadWriteData/IC_PHOTOS/") + item.FILE_9;
                                    <a href="@photoUrl" target="_blank">@item.FILE_9</a>
                                }

                            </td>
                            <td width="8%" valign="top" align="center">
                                @if (!string.IsNullOrEmpty(item.FILE_10))
                                {
                                    var photoUrl = Url.Content("~/ReadWriteData/IC_PHOTOS/") + item.FILE_10;
                                    <a href="@photoUrl" target="_blank">@item.FILE_10</a>
                                }

                            </td>
                        </tr>
                    }
                }
            </tbody>
        </table>
    </div>
</div>