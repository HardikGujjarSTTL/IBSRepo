@model IBS.Models.Reports.IEWiseTrainingReportModel

<div>
    <h3 class="center">@Model.Regions</h3>
    <h3 class="heading3">SUMMARY OF IE WISE TRAINING DETAILS</h3>
    <div class="tableOuter">
        <table class="borderTable" cellpadding="0" cellspacing="0">
            <tbody>
                <tr>
                    <th width="7%" valign="top" align="center"><b><font size="1" face="Verdana">S.No.</font></b></th>
                    <th width="7%" valign="top" align="center"><b><font size="1" face="Verdana">IE NAME.</font></b></th>
                    <th width="15%" valign="top" align="center"><b><font size="1" face="Verdana">EMP No.</font></b></th>
                    <th width="7%" valign="top" align="center"><b><font size="1" face="Verdana">CATEGORY</font></b></th>
                    <th width="8%" valign="top" align="center"><b><font size="1" face="Verdana">QUALIFICATION</font></b></th>
                    <th width="6%" valign="top" align="center"><b><font size="1" face="Verdana">TRAINING TYPE</font></b></th>
                    <th width="8%" valign="top" align="center"><b><font size="1" face="Verdana">TRAINING FEILD</font></b></th>
                    <th width="8%" valign="top" align="center"><b><font size="1" face="Verdana">COURSE NAME</font></b></th>
                    <th width="8%" valign="top" align="center"><b><font size="1" face="Verdana">COURSE INSTITUTE</font></b></th>
                    <th width="8%" valign="top" align="center"><b><font size="1" face="Verdana">COURSE DUR FR</font></b></th>
                    <th width="8%" valign="top" align="center"><b><font size="1" face="Verdana">COURSE DUR TO</font></b></th>
                    <th width="8%" valign="top" align="center"><b><font size="1" face="Verdana">CERTIFICATE</font></b></th>
                    <th width="8%" valign="top" align="center"><b><font size="1" face="Verdana">FEES</font></b></th>
                    <th width="8%" valign="top" align="center"><b><font size="1" face="Verdana">VALIDITY</font></b></th>
                </tr>
                @if (Model != null && Model.lstIEWISETRAINING != null && Model.lstIEWISETRAINING.Count > 0)
                {
                    var SrNo = 0;
                    @foreach (var item in Model.lstIEWISETRAINING)
                    {
                        SrNo = SrNo + 1;
                        <tr>
                            <td width="7%" valign="top" align="center"><font size="1" face="Verdana">@SrNo</font></td>
                            <td width="7%" valign="top" align="Left"><font size="1" face="Verdana">@item.NAME</font></td>
                            <td width="15%" valign="top" align="center"><font size="1" face="Verdana">@item.EMP_NO</font></td>
                            <td width="7%" valign="top" align="center"><font size="1" face="Verdana">@item.CATEGORY</font></td>
                            <td width="8%" valign="top" align="center"><font size="1" face="Verdana">@item.QUAL</font></td>
                            <td width="6%" valign="top" align="center"><font size="1" face="Verdana">@item.T_TYPE</font></td>
                            <td width="8%" valign="top" align="center"><font size="1" face="Verdana">@item.T_FEILD</font></td>
                            <td width="8%" valign="top" align="center"><font size="1" face="Verdana">@item.COURSE_NAME</font></td>
                            <td width="8%" valign="top" align="center"><font size="1" face="Verdana">@item.COURSE_INSTITUTE</font></td>
                            <td width="8%" valign="top" align="center"><font size="1" face="Verdana">@item.C_DUR_FR</font></td>
                            <td width="8%" valign="top" align="center"><font size="1" face="Verdana">@item.C_DUR_TO</font></td>
                            <td width="8%" valign="top" align="center"><font size="1" face="Verdana">@item.CERTIFICATE</font></td>
                            <td width="8%" valign="top" align="center"><font size="1" face="Verdana">@item.FEES</font></td>
                            <td width="8%" valign="top" align="center"><font size="1" face="Verdana">@item.VALIDITY</font></td>
                        </tr>
                    }
                }
            </tbody>
        </table>
    </div>
</div>


