@model IBS.Models.Reports.VendorFeedbackReportModel
<style>
    .borderTable th:nth-child(1){ width:3%;}
    .borderTable th:nth-child(3), .borderTable th:nth-child(4){ width:4.5%;}
</style>
<div>
    <h3 class="center">@Model.Regions</h3>
    <h3 class="heading3">Vendor Feedback Details</h3>
    <div class="tableOuter">
        <table class="borderTable" cellpadding="0" cellspacing="0">
            <tbody>
                <tr>
                    <th valign="top" align="center"><b>S.No.</b></th>
                    <th valign="top" align="center"><b>Vendor</b></th>
                    <th valign="top" align="center"><b>Region</b></th>
                    <th valign="top" align="center"><b>Period(YYYYMM)</b></th>
                    <th valign="top" align="center"><b>Promptness of RITES Regional Office in attending to P.O. & Call Registration</b></th>
                    <th valign="top" align="center"><b>Response of Call Desk in attending to Telephonic enquiries.</b></th>
                    <th valign="top" align="center"><b>Response of Inspecting Engineer for conducting inspection, timely attention and provision of advance intimation</b></th>
                    <th valign="top" align="center"><b>Behaviour of Inspecting Engineer with firm's representative during inspection and promptness in issue of inspection certificate</b></th>
                    <th valign="top" align="center"><b>Response of Controlling Managers to Firm's queries.</b></th>
                    <th valign="top" align="center"><b>RITES Lab response time.</b></th>
                    <th valign="top" align="center"><b>Quality of Information available on RITES QA Division website ritesinsp.com</b></th>
                    <th valign="top" align="center"><b>Promptness of RITES Finance Department in correction with Invoices/Bills/Receipts etc.</b></th>
                    <th valign="top" align="center"><b>Your overall experience with RITES QA Division, will you recommend others for RITES inspection.</b></th>
                    <th valign="top" align="center"><b>Suggestions for improvement in service.</b></th>
                </tr>
                @if (Model != null && Model.lstVendorFeedbackReport != null && Model.lstVendorFeedbackReport.Count > 0)
                {
                    var SrNo = 0;
                    decimal toFIELD_2 = 0, toFIELD_3 = 0, toFIELD_4 = 0, toFIELD_5 = 0, toFIELD_6 = 0, toFIELD_7 = 0, toFIELD_8 = 0, toFIELD_1 = 0;
                    @foreach (var item in Model.lstVendorFeedbackReport)
                    {
                        SrNo = SrNo + 1;
                        toFIELD_2 = toFIELD_2 + item.FIELD_2;
                        toFIELD_1 = toFIELD_1 + item.FIELD_1;
                        toFIELD_3 = toFIELD_3 + item.FIELD_3;
                        toFIELD_4 = toFIELD_4 + item.FIELD_4;
                        toFIELD_5 = toFIELD_5 + item.FIELD_5;
                        toFIELD_6 = toFIELD_6 + item.FIELD_6;
                        toFIELD_7 = toFIELD_7 + item.FIELD_7;
                        toFIELD_8 = toFIELD_8 + item.FIELD_8;
                        <tr>
                            <td valign="top" align="center">@SrNo</td>
                            <td valign="top" align="Left">@item.Vendor</td>
                            <td valign="top" align="center">@item.Region</td>
                            <td valign="top" align="center">@item.MTHYR_PERIOD</td>
                            <td valign="top" align="center">@item.FIELD_1</td>
                            <td valign="top" align="center">@item.FIELD_2</td>
                            <td valign="top" align="center">@item.FIELD_3</td>
                            <td valign="top" align="center">@item.FIELD_4</td>
                            <td valign="top" align="center">@item.FIELD_5</td>
                            <td valign="top" align="center">@item.FIELD_6</td>
                            <td valign="top" align="center">@item.FIELD_7</td>
                            <td valign="top" align="center">@item.FIELD_8</td>
                            <td valign="top" align="center">@item.FIELD_9</td>
                            <td valign="top" align="center">@item.FIELD_10</td>
                        </tr>
                    }
                    <tr>
                        <td valign="top" align="Left" colspan="2"><b>Totals</b></td>
                        <td valign="top" align="right"><b></b></td>
                        <td valign="top" align="right"><b></b></td>
                        <td valign="top" align="right"><b>@toFIELD_1</b></td>
                        <td valign="top" align="right"><b>@toFIELD_2</b></td>
                        <td valign="top" align="right"><b>@toFIELD_3</b></td>
                        <td valign="top" align="right"><b>@toFIELD_4</b></td>
                        <td valign="top" align="right"><b>@toFIELD_5</b></td>
                        <td valign="top" align="right"><b>@toFIELD_6</b></td>
                        <td valign="top" align="right"><b>@toFIELD_7</b></td>
                        <td valign="top" align="right"><b>@toFIELD_8</b></td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>


