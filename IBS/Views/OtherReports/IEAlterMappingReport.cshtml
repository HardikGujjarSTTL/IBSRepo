@model IBS.Models.Reports.IEAlterMappingReportModel

<div>
    <div class="tableOuter">
        <table class="borderTable" cellpadding="0" cellspacing="0">
            <tbody>
                <tr>
                    <th width="15%" valign="top" align="center"><b><font size="1" face="Verdana">S.No.</font></b></th>
                    <th width="15%" valign="top" align="center"><b><font size="1" face="Verdana">IE NAME</font></b></th>
                    <th width="15%" valign="top" align="center"><b><font size="1" face="Verdana">Alternate IE I	</font></b></th>
                    <th width="7%" valign="top" align="center"><b><font size="1" face="Verdana">Alternate IE II	</font></b></th>
                    <th width="8%" valign="top" align="center"><b><font size="1" face="Verdana">Alternate IE III</font></b></th>
                </tr>
                @if (Model != null && Model.lstIEAlterMappingReport != null && Model.lstIEAlterMappingReport.Count > 0)
                {
                    var SrNo = 0;
                    @foreach (var item in Model.lstIEAlterMappingReport)
                    {
                        SrNo = SrNo + 1;
                        <tr>
                            <td width="15%" valign="top" align="center"><font size="1" face="Verdana">@SrNo</font></td>
                            <td width="15%" valign="top" align="Left"><font size="1" face="Verdana">@item.IE_Name</font></td>
                            <td width="15%" valign="top" align="center"><font size="1" face="Verdana">@item.Altie_Name</font></td>
                            <td width="7%" valign="top" align="center"><font size="1" face="Verdana">@item.Altie_two_name</font></td>
                            <td width="8%" valign="top" align="center"><font size="1" face="Verdana">@item.Altie_three_name</font></td>
                        </tr>
                    }
                }
            </tbody>
        </table>
    </div>
</div>


