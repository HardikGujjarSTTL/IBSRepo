                            <td width="2%" valign="top" align="center"><font size="1" face="Verdana">@item.CO_NAME</font></td>
                            <td width="2%" valign="top" align="center"><font size="1" face="Verdana">@item.NI_WORK_PLAN_CD</font></td>
                            <td width="2%" valign="top" align="left"><font size="1" face="Verdana">@item.WORK_DATE</font></td>
