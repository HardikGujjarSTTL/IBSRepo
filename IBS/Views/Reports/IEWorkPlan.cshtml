                    <th width='8%' valign='top' align='center'><b><font size='1' face='Verdana'>CM Name</font></b></th>
                    <th width='7%' valign='top' align='center'><b><font size='1' face='Verdana'>Work Plan</font></b></th>
                    <th width='7%' valign='top' align='center'><b><font size='1' face='Verdana'>Work Date</font></b></th>
                    <th width='6%' valign='top' align='center'><b><font size='1' face='Verdana'>Time</font></b></th>                    
                </tr>
                @if (Model != null && Model.lstWorkPlan2 != null && Model.lstWorkPlan2.Count > 0)
