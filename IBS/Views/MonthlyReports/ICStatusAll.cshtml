@model IBS.Models.Reports.AllICStatusModel
<div>
    <h3 class="center">IC ACCOUNTAL for the Period <span class="orangeText"> @Model.Display_FromDate </span> To <span class="orangeText">@Model.Display_ToDate </span> (@Model.Region)</h3>
    <div class="tableOuter">
        <table class="borderTable" cellpadding="0" cellspacing="0">
            <tbody>
                <tr>
                    <th width="1%" valign="top" align="center"><b><font size="1" face="Verdana">IE Name</font></b></th>
                    <th width="1%" valign="top" align="center"><b><font size="1" face="Verdana">Book No.</font></b></th>
                    <th width="1%" valign="top" align="center"><b><font size="1" face="Verdana">Set No.</font></b></th>
                    <th width="1%" valign="top" align="center"><b><font size="1" face="Verdana">Status</font></b></th>                    
                </tr>
                @if (Model != null && Model.lstAllICStatus != null && Model.lstAllICStatus.Count > 0)
                {
                    @foreach (var item in Model.lstAllICStatus)
                    {
                        <tr>
                            <td width="1%" valign="top" align="Left"><font size="1" face="Verdana">@item.IE_NAME</font></td>
                            <td width="1%" valign="top" align="center"><font size="1" face="Verdana">@item.BK_NO</font></td>
                            <td width="1%" valign="top" align="center"><font size="1" face="Verdana">@item.SET_NO</font></td>
                            <td width="1%" valign="top" align="left"><font size="1" face="Verdana">@item.STATUS</font></td>                            
                        </tr>
                    }                    
                }
                else
                {
                    <tr>
                        <td colspan="4" class="noRecordGrid">No records found</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>    
</div>


