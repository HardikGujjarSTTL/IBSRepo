@model IBS.Models.Reports.PeriodWiseChecksheetReportModel

<div>
    <h3 class="heading3">@ViewBag.Regions</h3>
    <h3 class="heading3">Progress of Check sheets for the period : @ViewBag.frmdt To @ViewBag.todt</h3>
    <div class="tableOuter">
        <table class="borderTable" cellpadding="0" cellspacing="0">
            <tbody>
                <tr>
                    <th width="4%" valign="top" align="center"><b><font size="1" face="Verdana">S.No.</font></b></th>
                    <th width="15%" valign="top" align="center"><b><font size="1" face="Verdana">Item</font></b></th>
                    <th width="15%" valign="top" align="center"><b><font size="1" face="Verdana">Name of IE's/Shri</font></b></th>
                    <th width="7%" valign="top" align="center"><b><font size="1" face="Verdana">Name of CM's/Shri</font></b></th>
                </tr>
                @if (Model != null && Model.lstPeriodWiseChecksheet != null && Model.lstPeriodWiseChecksheet.Count > 0)
                {
                    var SrNo = 0;
                    @foreach (var item in Model.lstPeriodWiseChecksheet)
                    {
                        SrNo = SrNo + 1;
                        <tr>
                            <td width="4%" valign="top" align="center"><font size="1" face="Verdana">@SrNo</font></td>
                            <td width="15%" valign="top" align="Left"><font size="1" face="Verdana">@item.ITEM_DESC</font></td>
                            <td width="15%" valign="top" align="center"><font size="1" face="Verdana">@item.IE</font></td>
                            <td width="8%" valign="top" align="center"><font size="1" face="Verdana">@item.CO_NAME</font></td>
                        </tr>
                    }
                }
            </tbody>
        </table>
    </div>
</div>


