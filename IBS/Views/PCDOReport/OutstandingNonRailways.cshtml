@{
    ViewData["Title"] = "Outstanding Non-Railways";
}

<div>
    <div class="list-inner">
        <div class="tast-list">
            <h2>Outstanding Non-Railways</h2>
        </div>
    </div>
    @Html.Partial("_PCDOReportHeader")
</div>
@section scripts{
    <script type="text/javascript">
        function Search() {
            GetData();
        }
        function GetData() {
            var month = $("#ddlMonth").val();
            var Monthtext = $("#ddlMonth option:selected").text();
            var Year = $("#ddlYear").val();
            let myObject = { month, Year, Monthtext };
            var url = '@Url.Action("TableOutstandingNonRailways", "PCDOReport")?' + $.param(myObject);
            window.open(url, '_blank');
        }
    </script>
}