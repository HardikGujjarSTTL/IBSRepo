@{
    ViewData["Title"] = "Financial (Expenditure & Realization)";
}

<div>
    <div class="list-inner">
        <div class="tast-list">
            <h2>Financial (Expenditure & Realization)</h2>
        </div>
    </div>
    @Html.Partial("_PCDOReportHeader")

</div>
@section scripts{
    <script type="text/javascript">
        function Search() {
            GetData();
        }
        function GetData() {
            var month = $("#ddlMonth").val();
            var Monthtext = $("#ddlMonth option:selected").text();
            var Year = $("#ddlYear").val();
            let myObject = { month, Year, Monthtext };
            var url = '@Url.Action("TableFinancialExpenditureRealization", "PCDOReport")?' + $.param(myObject);
            window.open(url, '_blank');
        }
    </script>
}