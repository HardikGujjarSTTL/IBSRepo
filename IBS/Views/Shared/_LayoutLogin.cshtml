<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>IBS | Login</title>
    <link rel="shortcut icon" href="images/favicon.ico">
    <link href="~/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/css/font-awesome.min.css" type="text/css" rel="stylesheet">
    <link href="~/css/admin-style.css" type="text/css" rel="stylesheet">
    <link href="~/css/admin-responsive.css" type="text/css" rel="stylesheet">
    <link rel="stylesheet" href="~/lib/toastr.js/toastr.min.css" />
    <script src="~/lib/jquery/jquery.min.js"></script>
    <script src="~/js/crypto-js.js"></script>
    <script src="~/js/aes.js"></script>
</head>
<body class="loginPage">
    <partial name="_AlterPartial" />

    <section class="loginPageInfo">
        <div class="container">
            <div class="row my-5">
                <div class="col-md-6">
                    @*<div class="logo"> <a href="#" title="IBS"> <img src="~/images/logo-white.png" alt="IBS"> </a> </div>*@
                    <div class="logo"> <a href="#" title="IBS"> <img src="" alt="IBS"></a> </div>
                </div>
                <div class="col-md-6">
                    @RenderBody()
                </div>
            </div>
        </div>
        <div class="loginFooter">
            <div class="container">
                <p>Copyright &copy; 2023, IBS. All rights reserved.</p>
            </div>
        </div>
    </section>

    <script src="~/lib/toastr.js/toastr.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>

</html>