@model IBS.Models.Reports.HighValueInspReport

<div>
    <h3 class="heading3">@ViewBag.Regions</h3>
        <div class="tableOuter">
            <table class="borderTable" cellpadding="0" cellspacing="0">
                <tbody>
                    <tr>
                        <th width="15%" valign="top" align="center"><b><font size="1" face="Verdana">S.NO.</font></b></th>
                        <th width="15%" valign="top" align="center"><b><font size="1" face="Verdana">BILL NO.</font></b></th>
                        <th width="15%" valign="top" align="center"><b><font size="1" face="Verdana">BILL DT</font></b></th>
                        <th width="15%" valign="top" align="center"><b><font size="1" face="Verdana">IC DT</font></b></th>
                        <th width="15%" valign="top" align="center"><b><font size="1" face="Verdana">CASE NO.</font></b></th>
                        <th width="15%" valign="top" align="center"><b><font size="1" face="Verdana">ENG. NAME</font></b></th>
                        <th width="15%" valign="top" align="center"><b><font size="1" face="Verdana">VENDOR</font></b></th>
                        <th width="15%" valign="top" align="center"><b><font size="1" face="Verdana">CONSIGNEE</font></b></th>
                        <th width="15%" valign="top" align="center"><b><font size="1" face="Verdana">ITEM DESC.</font></b></th>
                        <th width="15%" valign="top" align="center"><b><font size="1" face="Verdana">MATERIAL VALUE</font></b></th>
                        <th width="15%" valign="top" align="center"><b><font size="1" face="Verdana">INSP. FEE</font></b></th>
                    </tr>
                    @if (Model != null && Model.lstValueInspList.Count > 0)
                    {
                         var SrNo = 0;
                        @foreach (var item in Model.lstValueInspList)
                        {
                            SrNo = SrNo + 1;
                            <tr>
                                <td width="15%" valign="top" align="Left"><font size="1" face="Verdana">@SrNo</font></td>
                                <td width="15%" valign="top" align="Left"><font size="1" face="Verdana">@item.BillNo</font></td>
                                <td width="15%" valign="top" align="Left"><font size="1" face="Verdana">@item.BillDate</font></td>
                                <td width="15%" valign="top" align="Left"><font size="1" face="Verdana">@item.ICDate</font></td>
                                <td width="15%" valign="top" align="Left"><font size="1" face="Verdana">@item.CaseNo</font></td>
                                <td width="15%" valign="top" align="Left"><font size="1" face="Verdana">@item.EngName</font></td>
                                <td width="15%" valign="top" align="Left"><font size="1" face="Verdana">@item.VENDOR</font></td>
                                <td width="15%" valign="top" align="Left"><font size="1" face="Verdana">@item.CONSIGNEE</font></td>
                                <td width="15%" valign="top" align="Left"><font size="1" face="Verdana">@item.ITEMDESC</font></td>
                                <td width="15%" valign="top" align="Left"><font size="1" face="Verdana">@item.MATERIALVALUE</font></td>
                                <td width="15%" valign="top" align="Left"><font size="1" face="Verdana">@item.INSPFEE</font></td>
                            </tr>
                        }
                    }
                </tbody>
            </table>
        </div>
</div>


