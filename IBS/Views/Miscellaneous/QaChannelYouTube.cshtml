
@{
    ViewData["Title"] = "QaChannelYouTube";
    Layout = "~/Views/Shared/_FrontendLayout.cshtml";
}

<div class="container" id="content">
    <div class="row m-0">
        <div class="col-lg-12">
            <div class="innerContent">
                <div class="list-inner">
                    <div class="tast-list">
                        <h2>QA Channel YouTube</h2>
                    </div>
                </div>
                <div class="accordion-body">

                    <div class="row">
                        <div class="col-lg-4">
                            <div class="galleryzoom galleryBoxInner">
                                <div class="galleryBox">
                                    <a href="https://www.youtube.com/watch?v=MRrztIJTnQc" title="Youtube Channel" class="youtubeChannel">
                                        <img src="~/frontend/photogallery/video-gallery-thumb.jpg" alt="Youtube Channel" />
                                    </a>
                                    <div class="galleryTitle"><span>QA Youtube Channel</span></div>
                                </div>
                            </div>
                        </div>

                    </div>

                </div>
            </div>
        </div>
    </div>
</div>
<script type="text/javascript">
    $(document).ready(function () {
        $('.youtubeChannel').magnificPopup({
            disableOn: 700,
            type: 'iframe',
            mainClass: 'mfp-fade',
            removalDelay: 160,
            preloader: false,

            fixedContentPos: false
        });
    });
</script>

