@model IBS.Models.Statement_IeVendorWiseModel

<div>
<style>
.borderTable th:nth-child(1){
    width:3%;
}

.borderTable th:nth-child(2)
{ 
    width:15%;
}

.borderTable th:nth-child(4),
.borderTable th:nth-child(5),
.borderTable th:nth-child(6),
.borderTable th:nth-child(7),
.borderTable th:nth-child(9),
.borderTable th:nth-child(10) {
    width: 6%;
}

.borderTable th:nth-child(3),
.borderTable th:nth-child(8) {
    width: 20%;
}
</style>
    @* <p align="center"><b><u>(@ViewBag.Region)</u></b></p> *@
    <h3 class="center">(@ViewBag.Region)</h3>



    <h3 class="center">Statement of IE & Vendor Wise Calls Cancelled during the Period <span class="orangeText"> @ViewBag.Frm </span> To <span class="orangeText">@ViewBag.To </span></h3>
        
       
   
    <br>
    <div class="tableOuter">
        <table class="borderTable" cellpadding="0" cellspacing="0">
            <tbody>
                <tr>
                    <th valign="top" align="center"><b>S.NO.</b></th>
                    <th valign="top" align="center"><b>IE Name</b></th>
                    <th valign="top" align="center"><b>Vendor</b></th>
                    <th valign="top" align="center"><b>Client</b></th>
                    <th valign="top" align="center"><b>Case No.</b></th>
                    <th valign="top" align="center"><b>Call Date</b></th>
                    <th valign="top" align="center"><b>Cancel Date</b></th>
                    <th valign="top" align="center"><b>Cancel Reason</b></th>
                    <th valign="top" align="center"><b>Cancel Status</b></th>
                    <th valign="top" align="center"><b>Cancel Charges</b></th>

                </tr>
                @if (Model != null && Model.statement_IeVendorWiseModels.Count > 0)
                {
                    var serialNumber = 1;
                    @foreach (var item in Model.statement_IeVendorWiseModels)
                    {

                        <tr>
                            <td valign="top" align="Left">@serialNumber</td>
                            <td valign="top" align="center">@item.IE_NAME</td>
                            <td valign="top" align="center">@item.VENDOR</td>
                            <td valign="top" align="center">@item.RLY_CD</td>
                            <td valign="top" align="center">@item.CASE_NO</td>
                            <td valign="top" align="center">@item.CALL_RECV_DT</td>
                            <td valign="top" align="center">@item.CANCEL_DT</td>
                            <td valign="top" align="center">@item.CANCEL_REASON</td>
                            <td valign="top" align="center">@item.CALL_CANCEL_STATUS</td>
                            <td valign="top" align="center">@item.CALL_CANCEL_CHARGES</td>
                        </tr>
                        serialNumber++;
                    }

                }
            </tbody>
        </table>
    </div>
</div>


