@model IBS.Models.VendorClusterReportModel

<div>
    <div class="tableOuter">
        <table class="borderTable" cellpadding="0" cellspacing="0">
            <tbody>
                <tr>
                    <th width="15%" valign="top" align="center"><b><font size="1" face="Verdana">S.No.</font></b></th>
                    <th width="15%" valign="top" align="center"><b><font size="1" face="Verdana">DEPARTMENT NAME</font></b></th>
                    <th width="15%" valign="top" align="center"><b><font size="1" face="Verdana">CLUSTER NAME</font></b></th>
                    <th width="7%" valign="top" align="center"><b><font size="1" face="Verdana">GEOGRAPHICAL PARTITION</font></b></th>
                    <th width="8%" valign="top" align="center"><b><font size="1" face="Verdana">VENDOR CODE</font></b></th>
                    <th width="6%" valign="top" align="center"><b><font size="1" face="Verdana">VENDOR NAME</font></b></th>
                    <th width="8%" valign="top" align="center"><b><font size="1" face="Verdana">ADDRESS</font></b></th>
                    <th width="8%" valign="top" align="center"><b><font size="1" face="Verdana">CITY</font></b></th>
                    <th width="8%" valign="top" align="center"><b><font size="1" face="Verdana">IE NAME</font></b></th>
                </tr>
                @if (Model != null && Model.lstVendorClusterList != null && Model.lstVendorClusterList.Count > 0)
                {
                    var SrNo = 0;
                    @foreach (var item in Model.lstVendorClusterList)
                    {
                        SrNo = SrNo + 1;
                        <tr>
                            <td width="15%" valign="top" align="center"><font size="1" face="Verdana">@SrNo</font></td>
                            <td width="15%" valign="top" align="Left"><font size="1" face="Verdana">@item.Department</font></td>
                            <td width="15%" valign="top" align="center"><font size="1" face="Verdana">@item.Cluster_name</font></td>
                            <td width="7%" valign="top" align="center"><font size="1" face="Verdana">@item.geographical_partition</font></td>
                            <td width="8%" valign="top" align="center"><font size="1" face="Verdana">@item.Vend_cd</font></td>
                            <td width="6%" valign="top" align="center"><font size="1" face="Verdana">@item.vendor</font></td>
                            <td width="8%" valign="top" align="center"><font size="1" face="Verdana">@item.vend_add1</font></td>
                            <td width="8%" valign="top" align="center"><font size="1" face="Verdana">@item.city</font></td>
                            <td width="8%" valign="top" align="center"><font size="1" face="Verdana">@item.Ie_name</font></td>
                        </tr>
                    }
                }
            </tbody>
        </table>
    </div>
</div>


