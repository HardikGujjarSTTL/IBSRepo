@using IBS.Helper;
@inject Microsoft.AspNetCore.Http.IHttpContextAccessor HttpContextAccessor;
@{
    string Region = ViewBag.Region;
    string ReportType = !string.IsNullOrEmpty(HttpContextAccessor.HttpContext.Request.Query["Action"]) ? Convert.ToString(HttpContextAccessor.HttpContext.Request.Query["Action"]) : "";
    string ReportTitle = string.Empty;

    if (ReportType == "U") ReportTitle = "Consignee Complaints For The Period";
    else if (ReportType == "COMPJI") ReportTitle = "Summary Of Consignee Complaints where JI Required";
    else if (ReportType == "TOPJI") ReportTitle = "JI Topsheet";
    else if (ReportType == "CORP") ReportTitle = "JI CONSIGNEE COMPLAINTS";
    else if (ReportType == "DCWACOMPS") ReportTitle = "DEFECT CODE WISE ANALYSIS OF COMPLAINTS";
    else if (ReportType == "COCOMPJI") ReportTitle = "Summarized Position Consignee Rejection (Region Wise)";
    else if (ReportType == "TOPNHIGH") ReportTitle = "HIGH VALUE INSPECTIONS";
    ViewData["Title"] = ReportTitle;
}

<div class="list-inner">
    <div class="tast-list">
        <h2>@ReportTitle</h2>
    </div>
</div>

