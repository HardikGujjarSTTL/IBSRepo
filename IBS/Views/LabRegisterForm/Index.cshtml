@model IBS.Models.LABREGISTERModel

@{
   
    string clsView = "";
    if (Model.LabID != "0")
    {
        
        clsView = "Permission_View";
    }
    ViewData["Title"] = "UPDATE LAB REGISTER";
}



<h2>@ViewData["Title"]</h2>

<section class="@clsView">
    <div class="task-listinput">
       
           @* <form data-ajax="true" asp-antiforgery="true" asp-controller="EFTEntry" asp-action="VoucherDetailsSave" data-ajax-method="post" data-ajax-complete="completed" id="frmEftDetails">*@
                <div class="accordion-body">
                    <div class="row my-0">
                        <div class="col-md-4 mb-4">
                            <label for="Reference">Lab Registration No.</label>
                            <input type="text" id="txtlabregno"  maxlength="300" />
                        </div>

                        <div class="col-md-4 mb-4">
                            <label for="Reference">Case No.</label>
                            <input type="text" id="txtcaseno"  maxlength="300" />
                           @* <span asp-validation-for="VCHR_DT" class="text-danger"></span>*@
                        </div>

                        <div class="col-md-4 mb-4">
                            <label for="Reference">Call Receive Date</label>
                            <input type="text" class="input datepicker" id="txtcallRDt"/>
                            @*  <input type="text" id="Chq_lbl" value="CASH" class="input" readonly="true">*@

                        </div>
                    <div class="savenext-btn">
                        
                        <button type="button" id="btnsr" class="save-btn active Permission_Save" onclick="grid();">Save</button>
                    </div>
                </div>
            <div id="gridshow" style="display:none;">
                <section class="table-section">
                    <div class="task-listinput">
                        <div class="dash-table">
                            <div class="table-responsive">
                                <table id="dtsearchdata" class="table">
                                    <thead>
                                        <tr>
                                            <th style="width:6%;">Select</th>
                                            <th style="width:13%;">Sample Reg No.</th>
                                            <th style="width:9%;">Case No.</th>
                                            <th style="width:9%;">Call Date</th>
                                            <th style="width:7%;">Call SNo.</th>
                                            <th style="width:5%;">IE</th>
                                            <th style="width:9%;">Status</th>


                                        </tr>
                                    </thead>
                                    <tbody id="tableBody">
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </section>
            </div>

          @*  </form>*@
        
    </div>
</section>


