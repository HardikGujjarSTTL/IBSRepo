@model IBS.Models.Reports.ICSubmissionModel
<div>
    <h3 class="center">IC Submission Report For the Period :  <span class="orangeText"> @Model.Display_FromDate </span> To <span class="orangeText">@Model.Display_ToDate </span> (@Model.Region)</h3>
    <div class="tableOuter">
        <table class="borderTable" cellpadding="0" cellspacing="0" style="width: 60%!important; margin-left: auto; margin-right: auto;">
            <tbody>
                <tr>
                    <th width="8%" valign="top" align="center"><b><font size="1" face="Verdana">S.No.</font></b></th>
                    <th width="22%" valign="top" align="center"><b><font size="1" face="Verdana">IC Submit Date</font></b></th>
                    <th width="30%" valign="top" align="center"><b><font size="1" face="Verdana">IE</font></b></th>
                    <th width="20%" valign="top" align="center"><b><font size="1" face="Verdana">BK NO.</font></b></th>
                    <th width="20%" valign="top" align="center"><b><font size="1" face="Verdana">SET No.</font></b></th>
                </tr>
                @if (Model != null && Model.lstICSubmission != null && Model.lstICSubmission.Count > 0)
                {
                    @foreach (var item in Model.lstICSubmission)
                    {
                        <tr>
                            <td width="8%" valign="top" align="center"><font size="1" face="Verdana">@item.ID</font></td>
                            <td width="22%" valign="top" align="center"><font size="1" face="Verdana">@item.Display_IC_SUBMIT_DATE</font></td>
                            <td width="30%" valign="top" align="center"><font size="1" face="Verdana">@item.IE_NAME</font></td>
                            <td width="20%" valign="top" align="center"><font size="1" face="Verdana">@item.BK_NO</font></td>
                            <td width="20%" valign="top" align="center"><font size="1" face="Verdana">@item.SET_NO</font></td>
                        </tr>
                    }
                }
                else
                {
                    <tr>
                        <td colspan="5" style="text-align:center; background: #f1f1f1 !important;"><font size="1" face="Verdana">No records found</font></td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>


