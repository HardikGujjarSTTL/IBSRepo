@model IBS.Models.Reports.TentativeInspectionFeeWisePendingCallsModel

<div>
    <h3 class="center">Tentative Inspection Fee Wise Pending Call : <span class="orangeText"> @Model.Display_FromDate </span> To <span class="orangeText">@Model.Display_ToDate </span> (@Model.Region)</h3>
    <div class="tableOuter">
        <table class="borderTable" cellpadding="0" cellspacing="0">
            <tbody>
                <tr>
                    <th width="5%" valign="top" align="center"><b><font size="1" face="Verdana">S.No.</font></b></th>
                    <th width="5%" valign="top" align="center"><b><font size="1" face="Verdana">Case No</font></b></th>
                    <th width="5%" valign="top" align="center"><b><font size="1" face="Verdana">SNo</font></b></th>
                    <th width="5%" valign="top" align="center"><b><font size="1" face="Verdana">Call Recieve Date</font></b></th>
                    <th width="10%" valign="top" align="center"><b><font size="1" face="Verdana">IE</font></b></th>
                    <th width="10%" valign="top" align="center"><b><font size="1" face="Verdana">CM</font></b></th>
                    <th width="10%" valign="top" align="center"><b><font size="1" face="Verdana">Client</font></b></th>
                    <th width="10%" valign="top" align="center"><b><font size="1" face="Verdana">Client Type</font></b></th>
                    <th width="5%" valign="top" align="center"><b><font size="1" face="Verdana">Pending Since(Current Date- Call_recv_Date)</font></b></th>
                    <th width="5%" valign="top" align="center"><b><font size="1" face="Verdana">Inspection Desire Date</font></b></th>
                    <th width="10%" valign="top" align="center"><b><font size="1" face="Verdana">Delivery Period</font></b></th>
                    <th width="10%" valign="top" align="center"><b><font size="1" face="Verdana">Material Value</font></b></th>
                    <th width="10%" valign="top" align="center"><b><font size="1" face="Verdana">Tentative Inspection Fee(For railway (0.167% after PO Dated 26/11/2022 or 0.9% upto PO Dated 26/11/2022)  and Non Railway .7%)</font></b></th>
                </tr>
                @if (Model != null && Model.lstTentativeInspectionFeeWisePendingCalls != null && Model.lstTentativeInspectionFeeWisePendingCalls.Count > 0)
                {
                    @foreach (var item in Model.lstTentativeInspectionFeeWisePendingCalls)
                    {
                        <tr>
                            <td width="5%" valign="top" align="center"><font size="1" face="Verdana">@item.ID</font></td>
                            <td width="5%" valign="top" align="center"><font size="1" face="Verdana">@item.CASE_NO</font></td>
                            <td width="5%" valign="top" align="center"><font size="1" face="Verdana">@item.CALL_SNO</font></td>
                            <td width="5%" valign="top" align="center"><font size="1" face="Verdana">@item.Display_CALL_RECV_DT</font></td>
                            <td width="10%" valign="top" align="center"><font size="1" face="Verdana">@item.IE_NAME</font></td>
                            <td width="10%" valign="top" align="center"><font size="1" face="Verdana">@item.CO_NAME</font></td>
                            <td width="10%" valign="top" align="center"><font size="1" face="Verdana">@item.CLIENT_RLY</font></td>
                            <td width="10%" valign="top" align="center"><font size="1" face="Verdana">@item.RLY</font></td>
                            <td width="5%" valign="top" align="center"><font size="1" face="Verdana">@decimal.Truncate(item.PENDING_SINCE)</font></td>
                            <td width="5%" valign="top" align="center"><font size="1" face="Verdana">@item.Display_INSP_DESIRE_DATE</font></td>
                            <td width="10%" valign="top" align="center"><font size="1" face="Verdana">@item.Display_EXT_DELV_DATE</font></td>
                            <td width="10%" valign="top" align="center"><font size="1" face="Verdana">@item.MAT_VALUE</font></td>
                            <td width="10%" valign="top" align="center"><font size="1" face="Verdana">@item.INSP_FEE</font></td>
                        </tr>
                    }
                }
                else
                {
                    <tr>
                        <td colspan="13" class="noRecordGrid">No records found</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>





